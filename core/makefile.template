# $(DEP_FILE) is a .dep file generated by fort_depend.py
DEP_FILE = nekbox.dep
                        
################################################################################
all : nek5000 $(DEP_FILE) 

# create fortran dependencies ##################################################
$(DEP_FILE):
	@echo "Making dependencies!"
	python $S/fort_depend.py -w -o ./$(DEP_FILE) -b $(OBJDIR) -f $S/*.F90 *.F90

ifneq ($(MAKECMDGOALS),clean)
-include $(DEP_FILE)
endif


objdir: 
	@mkdir $(OBJDIR) 2>/dev/null; cat /dev/null 

nek5000:	objdir $(NOBJS)
	$(F77) -c $(FL2) $(CASEDIR)/${CASENAME}.F90 $(MOABNEK_INCLUDES) $(VISITNEK_INCLUDES) $(IMESH_INCLUDES) -o ${OBJDIR}/${CASENAME}.o 
	$(F77) -o ${BINNAME} $G ${OBJDIR}/${CASENAME}.o $(NOBJS) $(lFLAGS)
	@if test -f ${BINNAME}; then \
	echo "#############################################################"; \
	echo "#                  Compilation successful!                  #"; \
	echo "#############################################################"; \
        size ${BINNAME}; \
        echo ""; \
        echo "$(NEK_WARN)"; \
	else \
	echo -e "\033[1;31;38m" "ERROR: Compilation failed!"; \
	echo -e "\033[0m"; \
	fi
ifeq ($(IFMPI),false) 
	@rm -rf $S/mpif.h
endif

lib:	objdir $(NOBJS)
	$(AR) cru ${LIBNAME} $(NOBJS)
	ranlib ${LIBNAME}

.PHONY : doc clean

clean:
	rm -rf *.mod ./obj $(DEP_FILE) ${BINNAME}
ifeq ($(IFMPI),false) 
	@rm -rf $S/mpif.h
endif

doc:
	rm -rf ./doc
	doxygen Doxyfile

pub_doc: doc
	cd ./doc/html/ && \
	git init && \
	git checkout -b gh-pages && \
	git add . && \
	git remote add origin git@github.com:maxhutch/nek.git && \
	git commit -m "Updating Doxygen" && \
	git push origin gh-pages --force 

